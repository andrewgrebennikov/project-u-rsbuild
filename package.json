{
  "name": "project-u-rsbuild",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start": "concurrently \"npm run start:server\" \"npm run start:dev\"",
    "start:dev": "rsbuild dev --open --port 3000",
    "start:server": "node json-server/index.js",
    "build:prod": "rsbuild build --env-mode .env.production",
    "build:dev": "rsbuild build --env-mode .env.development",
    "preview:prod": "rsbuild preview --env-mode .env.production",
    "preview:dev": "rsbuild preview --env-mode .env.development",
    "prettier": "prettier --write ./**/*.{js,jsx,ts,tsx}",
    "lint:eslint": "eslint \"./**/*.{js,jsx,ts,tsx}\"",
    "lint:eslint:fix": "eslint \"./**/*.{js,jsx,ts,tsx}\" --fix",
    "lint:stylelint": "stylelint \"./**/*.scss\"",
    "lint:stylelint:fix": "stylelint \"./**/*.scss\" --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "axios": "^1.6.7",
    "classix": "^2.1.36",
    "i18next": "^23.10.1",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.5.0",
    "modern-normalize": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@conarti/eslint-plugin-feature-sliced": "^1.0.5",
    "@rsbuild/core": "^0.4.14",
    "@rsbuild/plugin-react": "^0.4.14",
    "@rsbuild/plugin-svgr": "^0.4.14",
    "@types/node": "^20.11.26",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-i18next": "^6.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "json-server": "^0.17.4",
    "prettier": "^3.2.5",
    "stylelint": "^16.2.1",
    "stylelint-config-clean-order": "^5.4.1",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-prettier": "^5.0.0",
    "typescript": "^5.4.2"
  },
  "volta": {
    "node": "20.11.1"
  }
}
